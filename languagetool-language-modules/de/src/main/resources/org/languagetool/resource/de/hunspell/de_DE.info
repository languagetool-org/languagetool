# Dictionary properties for de_DE.dict

fsa.dict.separator=+
fsa.dict.encoding=utf-8

fsa.dict.speller.replacement-pairs=ß ss, ss ß, ae ä, e ä, ä e, oe ö, ö oe, ue ü, Ae Ä, Oe Ö, Ue Ü, Rh R, R Rh, rh r, th t, g ch, ch g, f ph, ph f, Ph F, F Ph, f pf, pf f, Pf F, F Pf, mft nft, F V, W V, f v, w v, i y, j y, ü y, J Y, ee é, é ee, x chs, scht st, Scht St, schp sp, Schp Sp, d t, t d, o oh, oh o, alje aille, allje aille

#fsa.dict.speller.runon-words=false
# ignore-diacritics=false speeds up building the suggestions by a factor of about 2:
fsa.dict.speller.ignore-diacritics=false
fsa.dict.frequency-included=true

fsa.dict.encoder=SUFFIX
