#
# Dictionary properties.
#

fsa.dict.separator=_
fsa.dict.encoding=utf-8

fsa.dict.encoder=SUFFIX

fsa.dict.speller.locale=en_GB
fsa.dict.speller.ignore-diacritics=true
fsa.dict.speller.replacement-pairs=ph f, f ph, gh f, ff ugh, uf ough, uff ough, k ch, ch k, dg j, j dg, w ugh, ness ity, leness ility, ness ivity, eness ity, og ogue, ck qu, ck que, ght t, ts ce, ce ts, ninties 1990s
fsa.dict.frequency-included=true
fsa.dict.speller.ignore-all-uppercase=false
fsa.dict.speller.ignore-camel-case=false
