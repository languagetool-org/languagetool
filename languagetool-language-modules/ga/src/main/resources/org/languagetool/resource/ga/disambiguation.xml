    <rulegroup id="A_VERBAL_NOUN_VOWEL" name="á [verbal noun]">
        <rule>
            <pattern>
                <marker>
                    <token postag="Prep:Poss:3P:Sg:Masc:Obj">á</token>
                    <token postag="Verbal:Noun:V[TI][ID]?:(?:Vow:)?Len" postag_regexp="yes" regexp="yes">[aeiouáéíóú].*</token>
                </marker>
            </pattern>
            <disambig action="filterall"></disambig>
        </rule>
        <rule>
            <pattern>
                <marker>
                    <token postag="Prep:Poss:3P:Sg:Fem:Obj">á</token>
                    <token postag="Verbal:Noun:V[TI][ID]?:(?:Vow:)?hPref" postag_regexp="yes" regexp="yes">h[aeiouáéíóú].*</token>
                </marker>
            </pattern>
            <disambig action="filterall"></disambig>
        </rule>
        <rule>
            <pattern>
                <marker>
                    <token postag="Prep:Poss:3P:Pl:Obj">á</token>
                    <token postag="Verbal:Noun:V[TI][ID]?:(?:Vow:)?Ecl" postag_regexp="yes" regexp="yes">n-[aeiouáéíóú].*</token>
                </marker>
            </pattern>
            <disambig action="filterall"></disambig>
        </rule>
    </rulegroup>
    <rule name="ar mhaithe le" id="ar_mhaithe_le">
