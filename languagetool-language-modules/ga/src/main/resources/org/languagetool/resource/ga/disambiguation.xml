    <rule name="de Valera" id="de_Valera">
        <pattern>
            <marker><token>de</token></marker>
            <token regexp="yes">Valera|Brún|hÍde|Niro|Róiste|Cléir|Spáinn|Bhaldraithe</token>
        </pattern>
        <disambig postag="Part:Pat"/>
