    <rulegroup id="A_HAON" name="a haon">
        <rule>
            <pattern>
                <marker>
                    <token postag="Part:Nm">a</token>
                    <token regexp="yes" postag="Num:Card:hPref">haon|hochtó|hocht</token>
                </marker>
            </pattern>
            <disambig action="filterall"></disambig>
        </rule>
        <rule>
            <pattern>
                <marker>
                    <token postag="Part:Nm">a</token>
                    <token regexp="yes" postag="Num:Card">dó|trí|ceathair|cúig|sé|seacht|naoi|deich</token>
                </marker>
            </pattern>
            <disambig action="filterall"></disambig>
        </rule>
        <rule>
            <pattern>
                <marker>
                    <token>a</token>
                </marker>
                <token regexp="yes">[0-9]+</token>
            </pattern>
            <disambig action="filter" postag="Part:Nm"/>
        </rule>
        <rule>
            <pattern>
                <token>a</token>
                <marker>
                    <token regexp="yes">[0-9]+</token>
                </marker>
            </pattern>
            <disambig action="add"><wd pos="Num:Dig"/></disambig>
        </rule>
    </rulegroup>
    <rule id="DE_HAOINE" name="Dé hAoine">
