    <rule id="GO_RAIBH" name="go raibh">
        <pattern>
            <marker>
                <token postag="Prep:Vb:.*" postag_regexp="yes">go</token>
                <token postag="Verb:.*" postag_regexp="yes">raibh</token>
            </marker>
        </pattern>
        <disambig action="filterall"></disambig>
    </rule>
