<rules lang="sr"
	xsi:noNamespaceSchemaLocation="../../../../../../../../../languagetool-core/src/main/resources/org/languagetool/rules/rules.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<unification feature="number">
		<equivalence type="sin">
			<token></token>
		</equivalence>
		<equivalence type="plu">
			<token></token>
		</equivalence>
	</unification>
	
	<unification feature="case">
		<equivalence type="nom">
			<token></token>
		</equivalence>
		<equivalence type="gen">
			<token></token>
		</equivalence>
		<equivalence type="dat">
			<token></token>
		</equivalence>
		<equivalence type="acc">
			<token></token>
		</equivalence>
		<equivalence type="voc">
			<token></token>
		</equivalence>
		<equivalence type="ins">
			<token></token>
		</equivalence>
		<equivalence type="loc">
			<token></token>
		</equivalence>
	</unification>

	<unification feature="gender">
		<equivalence type="mas"> <!-- masculine -->
			<token></token>
		</equivalence>
		<equivalence type="fem"> <!-- feminine -->
			<token></token>
		</equivalence>
		<equivalence type="neu"> <!-- neuter -->
			<token></token>
		</equivalence>
		<equivalence type="gen_plu"> <!-- Gender plural -->
			<token></token>
		</equivalence>
	</unification>

	<unification feature="tense">
		<equivalence type="inf"> <!-- infinitive -->
			<token></token>
		</equivalence>
		<equivalence type="pre"> <!-- present -->
			<token></token>
		</equivalence>
	</unification>
</rules>
